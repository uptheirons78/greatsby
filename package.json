{
  "name": "greatsby",
  "private": true,
  "description": "Blog Website built with Gatsby and Contentful Api",
  "version": "0.1.0",
  "author": "Mauro Bono <mauro@maurobono.com> (https://www.maurobono.com)",
  "license": "MIT",
  "scripts": {
