    "gatsby-transformer-remark": "^2.6.21",
    "node-sass": "^4.12.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "env-cmd": "^10.0.1",
    "prettier": "^1.18.2"
